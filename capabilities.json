{
    "dataRoles": [
        {
            "displayName": "Time Hierarchy",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Measure Data",
            "name": "measure",
            "kind": "Measure"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 1
                    },
                    "measure": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "sample": {
                            "count": 3000
                        }
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "objects": {
        "SPCSettings":{
            "properties":{
                "direction": {
                    "type": {
                        "enumeration" : [{"value":  "1", "displayName": "Up"}, 
                                         {"value":  "0", "displayName": "No Direction"}, 
                                         {"value": "-1", "displayName": "Down"}]
                    }
                },
                "show": {
                    "type": {
                        "bool": true
                    }
                },
                "showMarker": {
                    "type": {
                        "bool": true
                    }
                },
                "outlier": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "run": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "oneside": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }

            }
        },
        "enableAxis": {
            "properties": {
                "show": {
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "enableYAxis": {
            "properties": {
                "show": {
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "time": {
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    },
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "Tooltips"
        ]
    },
    "supportsLandingPage": false,
    "drilldown": {
        "roles": [
            "category"
        ]
    },
    "privileges": []
}